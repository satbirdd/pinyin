<p id="notice"><%= notice %></p>

<h1>Listing Test Paper Letters</h1>

<table>
  <thead>
    <tr>
      <th>Letter</th>
      <th>Test paper</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_paper_letters.each do |test_paper_letter| %>
      <tr>
        <td><%= test_paper_letter.letter %></td>
        <td><%= test_paper_letter.test_paper %></td>
        <td><%= link_to 'Show', test_paper_letter %></td>
        <td><%= link_to 'Edit', edit_test_paper_letter_path(test_paper_letter) %></td>
        <td><%= link_to 'Destroy', test_paper_letter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test paper letter', new_test_paper_letter_path %>
